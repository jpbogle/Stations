{
    "extends": "airbnb",
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": false,
        "codeFrame": false
    },
    "parser": "babel-eslint",
    "globals": {
        "document": false,
        "window": false,
        "fetch": false
    },
    "rules": {
        "react/prefer-stateless-function": "off",
        "react/no-unused-prop-types": "off",
        "class-methods-use-this": "off",
        "import/no-named-default": "off"
    }
}
